{
  "name": "shopping-list",
  "version": "1.0.0",
  "description": "A Shopping List Built on top of the MERN Stack",
  "main": "server.js",
  "keywords": [
    "React",
    "Cryce",
    "Truly",
    "MongoDb",
    "Express"
  ],
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Cryce Truly",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.17.5"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.0",
    "express": "^4.16.3",
    "mongoose": "^5.1.7",
    "bcryptjs": "*",
    "bluebird": "*",
    "passport": "*",
    "passport-jwt": "*",
    "jsonwebtoken": "*"
  }
}
